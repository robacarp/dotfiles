[user]
	name = 
	email = 
	signingkey = A384CFD09D54E693
[color]
  diff = auto
  status = auto
  branch = auto
[push]
	default = simple
[alias]
  co = checkout
  br = branch
  s = status
[core]
  editor = /usr/bin/vim
	excludesfile = /Users/robert/.gitignore_global
	autocrlf = input
[color "status"]
	branch = magenta
[merge]
	tool = opendiff
[commit]
	template = /Users/robert/.stCommitMsg
[difftool "sourcetree"]
	cmd = opendiff \"$LOCAL\" \"$REMOTE\"
	path = 
[mergetool "sourcetree"]
	cmd = /Users/robert/Applications/Sourcetree.app/Contents/Resources/opendiff-w.sh \"$LOCAL\" \"$REMOTE\" -ancestor \"$BASE\" -merge \"$MERGED\"
	trustExitCode = true
[pull]
	ff = only
